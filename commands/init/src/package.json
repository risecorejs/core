{
  "name": "risecorejs-base-template",
  "version": "1.0.0",
  "description": "Risecorejs - The Node.js Framework",
  "main": "config.js",
  "private": true,
  "scripts": {
    "tsc": "tsc --watch",
    "dev": "risecorejs dev",
    "start": "risecorejs start",
    "make:migrations": "risecorejs make:migrations",
    "db:migrate": "sequelize db:migrate",
    "db:seeder": "sequelize db:seed:all",
    "db:rollback": "sequelize db:migrate:undo:all"
  },
  "dependencies": {
    "@risecorejs/core": "^3.0.21",
    "@risecorejs/helpers": "^3.0.10",
    "@risecorejs/middleware": "^3.0.3",
    "@risecorejs/crud-builder": "^3.0.2",
    "sequelize": "^6.6.5",
    "sequelize-cli": "^6.2.0"
  },
  "devDependencies": {
    "@types/node": "^18.7.11",
    "prettier": "^2.3.2",
    "typescript": "^4.7.4"
  }
}
